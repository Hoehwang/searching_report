        <ClinVarAccession Accession="SCV001408294" DateUpdated="2024-02-28" DateCreated="2020-07-16" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
