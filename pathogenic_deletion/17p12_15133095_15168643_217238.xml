        <ClinVarAccession Accession="SCV000759656" DateUpdated="2024-02-14" DateCreated="2018-05-28" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
