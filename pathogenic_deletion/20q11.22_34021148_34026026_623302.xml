        <ClinVarAccession Accession="SCV000029100" DateUpdated="2024-07-07" DateCreated="2013-04-04" Type="SCV" Version="2" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2003-08-15">
