        <ClinVarAccession Accession="SCV001339011" DateUpdated="2020-06-22" DateCreated="2020-06-22" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2020-03-02">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
