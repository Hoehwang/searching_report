        <ClinVarAccession Accession="SCV000829360" DateUpdated="2024-02-20" DateCreated="2018-10-10" Type="SCV" Version="7" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2024-01-01">
