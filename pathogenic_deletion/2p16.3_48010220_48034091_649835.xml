        <ClinVarAccession Accession="SCV001176654" DateUpdated="2024-05-01" DateCreated="2020-03-16" Type="SCV" Version="4" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
