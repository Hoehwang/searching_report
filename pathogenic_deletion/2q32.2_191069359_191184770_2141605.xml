        <ClinVarAccession Accession="SCV003447940" DateUpdated="2024-02-14" DateCreated="2023-02-07" Type="SCV" Version="2" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
