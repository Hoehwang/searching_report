        <ClinVarAccession Accession="SCV004007817" DateUpdated="2024-05-01" DateCreated="2023-07-08" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
