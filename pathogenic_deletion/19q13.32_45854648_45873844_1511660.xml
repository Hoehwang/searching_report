        <ClinVarAccession Accession="SCV002500596" DateUpdated="2022-04-23" DateCreated="2022-04-23" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-03-23">
