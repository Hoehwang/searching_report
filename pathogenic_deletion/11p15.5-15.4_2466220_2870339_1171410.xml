        <ClinVarAccession Accession="SCV001734380" DateUpdated="2021-06-19" DateCreated="2021-06-19" Type="SCV" Version="1" SubmitterName="Color Diagnostics, LLC DBA Color Health" OrgID="505849" OrganizationCategory="laboratory" />
