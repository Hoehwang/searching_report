        <ClinVarAccession Accession="SCV000651240" DateUpdated="2024-02-20" DateCreated="2017-12-26" Type="SCV" Version="9" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2024-01-30">
