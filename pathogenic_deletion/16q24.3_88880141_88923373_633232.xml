        <ClinVarAccession Accession="SCV000941325" DateUpdated="2024-02-14" DateCreated="2019-08-14" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-01-28">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
