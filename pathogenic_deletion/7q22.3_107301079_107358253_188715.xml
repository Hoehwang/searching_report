        <ClinVarAccession Accession="SCV000025274" DateUpdated="2016-10-07" DateCreated="2013-04-04" Type="SCV" Version="3" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2008-01-01">
