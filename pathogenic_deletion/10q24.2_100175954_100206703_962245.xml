        <ClinVarSubmissionID localKey="NM_000195.4:c.1932delC|MedGen:C0079504" submittedAssembly="GRCh37" />
        <ClinVarAccession Accession="SCV001442761" DateUpdated="2020-11-12" DateCreated="2020-11-12" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
