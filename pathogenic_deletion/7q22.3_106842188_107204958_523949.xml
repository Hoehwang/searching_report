        <ClinVarAccession Accession="SCV001371820" DateUpdated="2020-07-16" DateCreated="2020-07-16" Type="SCV" Version="1" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2020-07-09">
