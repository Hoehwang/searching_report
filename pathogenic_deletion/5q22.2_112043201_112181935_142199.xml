        <ClinVarAccession Accession="SCV000253728" DateUpdated="2024-02-20" DateCreated="2015-10-11" Type="SCV" Version="9" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
