        <ClinVarAccession Accession="SCV003687181" DateUpdated="2024-05-01" DateCreated="2023-02-07" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
