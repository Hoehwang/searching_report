        <ClinVarAccession Accession="SCV004595934" DateUpdated="2024-02-28" DateCreated="2024-02-28" Type="SCV" Version="1" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-10-24">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
