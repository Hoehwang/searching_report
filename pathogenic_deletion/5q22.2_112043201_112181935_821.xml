      <ClinicalAssertion ID="21013" SubmissionDate="2013-04-04" DateLastUpdated="2013-04-04" DateCreated="2013-04-04">
        <ClinVarSubmissionID localKey="611731.0028_FAMILIAL ADENOMATOUS POLYPOSIS 1, ATTENUATED" title="APC, 4-BP DEL, EX15_FAMILIAL ADENOMATOUS POLYPOSIS 1, ATTENUATED" />
        <ClinVarAccession Accession="SCV000021013" DateUpdated="2013-04-04" DateCreated="2013-04-04" Type="SCV" Version="1" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
