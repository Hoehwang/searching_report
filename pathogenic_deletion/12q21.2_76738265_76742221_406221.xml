        <ClinVarAccession Accession="SCV000544764" DateUpdated="2024-02-14" DateCreated="2017-04-17" Type="SCV" Version="8" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
