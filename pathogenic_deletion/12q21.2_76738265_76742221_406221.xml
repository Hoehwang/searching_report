        <ClinVarAccession Accession="SCV000699623" DateUpdated="2017-04-17" DateCreated="2017-04-17" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2017-08-22">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
