                <ID Source="PubMed">10655546</ID>
              </Citation>
              <Citation>
                <ID Source="PubMed">15621878</ID>
              </Citation>
              <XRef DB="OMIM" ID="617315" Type="MIM" />
              <XRef DB="OMIM" ID="231300" Type="MIM" />
            </ObservedData>
