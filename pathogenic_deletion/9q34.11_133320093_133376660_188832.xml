        <ClinVarAccession Accession="SCV000915271" DateUpdated="2019-05-24" DateCreated="2019-05-24" Type="SCV" Version="1" SubmitterName="Illumina Laboratory Services, Illumina" OrgID="504895" OrganizationCategory="laboratory" />
