        <ClinVarAccession Accession="SCV000663336" DateUpdated="2024-05-01" DateCreated="2018-01-01" Type="SCV" Version="5" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2020-10-23">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
