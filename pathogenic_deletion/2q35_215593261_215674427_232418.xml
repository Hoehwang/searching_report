        <ClinVarAccession Accession="SCV000779100" DateUpdated="2018-07-09" DateCreated="2018-07-09" Type="SCV" Version="1" SubmitterName="GeneDx" OrgID="26957" OrganizationCategory="laboratory" OrgAbbreviation="GDX" />
