        <ClinVarAccession Accession="SCV000663481" DateUpdated="2024-05-01" DateCreated="2018-01-01" Type="SCV" Version="5" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
