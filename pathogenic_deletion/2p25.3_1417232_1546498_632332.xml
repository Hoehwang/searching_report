        <ClinVarAccession Accession="SCV000915862" DateUpdated="2019-05-27" DateCreated="2019-05-27" Type="SCV" Version="1" SubmitterName="Illumina Laboratory Services, Illumina" OrgID="504895" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2018-10-05">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
