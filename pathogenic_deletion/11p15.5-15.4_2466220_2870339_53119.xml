        <ClinVarAccession Accession="SCV001350862" DateUpdated="2022-01-12" DateCreated="2020-06-22" Type="SCV" Version="2" SubmitterName="Color Diagnostics, LLC DBA Color Health" OrgID="505849" OrganizationCategory="laboratory" />
