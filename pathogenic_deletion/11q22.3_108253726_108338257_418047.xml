        <ClinVarAccession Accession="SCV001178753" DateUpdated="2022-11-29" DateCreated="2020-03-16" Type="SCV" Version="3" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
