        <ClinVarAccession Accession="SCV000934521" DateUpdated="2019-08-14" DateCreated="2019-08-14" Type="SCV" Version="1" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
