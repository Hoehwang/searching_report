        <ClinVarAccession Accession="SCV000216463" DateUpdated="2024-05-01" DateCreated="2015-03-24" Type="SCV" Version="7" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2021-05-14">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
