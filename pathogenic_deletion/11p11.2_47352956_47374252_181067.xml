        <ClinVarAccession Accession="SCV002709018" DateUpdated="2024-05-01" DateCreated="2022-11-29" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-05-05">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
