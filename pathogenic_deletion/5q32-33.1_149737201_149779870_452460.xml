        <ClinVarAccession Accession="SCV000776782" DateUpdated="2024-02-14" DateCreated="2018-05-28" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
