        <ClinVarAccession Accession="SCV003792848" DateUpdated="2023-11-11" DateCreated="2023-02-13" Type="SCV" Version="2" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-06-10">
