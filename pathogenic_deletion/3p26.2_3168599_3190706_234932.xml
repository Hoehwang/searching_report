        <ClinVarAccession Accession="SCV001497543" DateUpdated="2024-02-14" DateCreated="2021-03-07" Type="SCV" Version="3" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
