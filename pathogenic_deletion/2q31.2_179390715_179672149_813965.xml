        <ClinVarAccession Accession="SCV004053317" DateUpdated="2024-05-01" DateCreated="2023-10-28" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
