        <ClinVarAccession Accession="SCV003915661" DateUpdated="2024-02-20" DateCreated="2023-04-15" Type="SCV" Version="2" SubmitterName="Clinical Genomics, Uppaluri K&amp;H Personalized Medicine Clinic" OrgID="508223" OrganizationCategory="clinic" OrgAbbreviation="K&amp;H CLINIC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2024-02-14">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Likely risk allele</GermlineClassification>
          <Citation Type="general">
            <ID Source="PubMed">19417036</ID>
