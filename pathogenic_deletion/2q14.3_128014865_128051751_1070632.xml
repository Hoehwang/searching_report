        <ClinVarAccession Accession="SCV002600481" DateUpdated="2022-11-19" DateCreated="2022-11-19" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-10-13">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
