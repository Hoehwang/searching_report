        <ClinVarAccession Accession="SCV000835138" DateUpdated="2024-02-14" DateCreated="2018-10-10" Type="SCV" Version="7" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
