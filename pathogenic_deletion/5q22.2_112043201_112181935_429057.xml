        <Classification DateLastEvaluated="2020-08-20">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Pathogenic</GermlineClassification>
          <Citation>
            <ID Source="PubMed">9824584</ID>
          </Citation>
          <Citation>
            <ID Source="PubMed">1316610</ID>
          </Citation>
          <Citation>
            <ID Source="PubMed">27081525</ID>
          </Citation>
          <Citation>
