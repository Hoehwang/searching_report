        <ClinVarAccession Accession="SCV000253731" DateUpdated="2024-02-20" DateCreated="2015-10-11" Type="SCV" Version="7" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
