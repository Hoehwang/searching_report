        <ClinVarAccession Accession="SCV003551091" DateUpdated="2024-05-01" DateCreated="2023-02-07" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2020-12-03">
