        <ClinVarAccession Accession="SCV000992304" DateUpdated="2020-10-23" DateCreated="2019-09-09" Type="SCV" Version="2" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2020-10-19">
