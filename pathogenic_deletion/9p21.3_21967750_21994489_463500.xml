        <ClinVarAccession Accession="SCV004361300" DateUpdated="2024-02-14" DateCreated="2024-02-14" Type="SCV" Version="1" SubmitterName="Color Diagnostics, LLC DBA Color Health" OrgID="505849" OrganizationCategory="laboratory" />
