        <ClinVarAccession Accession="SCV004298801" DateUpdated="2024-02-14" DateCreated="2024-02-14" Type="SCV" Version="1" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
