        <ClinVarAccession Accession="SCV000649362" DateUpdated="2024-02-28" DateCreated="2017-12-26" Type="SCV" Version="7" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-07-06">
