        <ClinVarAccession Accession="SCV000273322" DateUpdated="2024-05-01" DateCreated="2016-05-29" Type="SCV" Version="7" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2021-04-21">
