        <ClinVarAccession Accession="SCV000563930" DateUpdated="2017-04-17" DateCreated="2017-04-17" Type="SCV" Version="2" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
