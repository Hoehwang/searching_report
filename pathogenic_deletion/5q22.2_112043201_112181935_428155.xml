        <ClinVarAccession Accession="SCV000579898" DateUpdated="2024-05-01" DateCreated="2017-06-25" Type="SCV" Version="7" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
