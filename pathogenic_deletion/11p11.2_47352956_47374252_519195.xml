        <ClinVarAccession Accession="SCV002235520" DateUpdated="2024-02-14" DateCreated="2022-03-28" Type="SCV" Version="3" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
