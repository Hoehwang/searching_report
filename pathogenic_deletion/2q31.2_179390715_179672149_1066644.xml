        <ClinVarAccession Accession="SCV001575087" DateUpdated="2024-02-28" DateCreated="2021-05-10" Type="SCV" Version="5" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
