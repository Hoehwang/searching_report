        <ClinVarAccession Accession="SCV000025455" DateUpdated="2015-05-23" DateCreated="2013-04-04" Type="SCV" Version="2" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
