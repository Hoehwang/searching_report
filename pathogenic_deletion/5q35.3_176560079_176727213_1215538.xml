        <ClinVarAccession Accession="SCV002107886" DateUpdated="2023-02-07" DateCreated="2022-03-28" Type="SCV" Version="2" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2021-09-15">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
