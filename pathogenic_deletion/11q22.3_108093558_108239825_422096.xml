        <ClinVarAccession Accession="SCV000687406" DateUpdated="2024-02-14" DateCreated="2018-02-19" Type="SCV" Version="3" SubmitterName="Color Diagnostics, LLC DBA Color Health" OrgID="505849" OrganizationCategory="laboratory" />
