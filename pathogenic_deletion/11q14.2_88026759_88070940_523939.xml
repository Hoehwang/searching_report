        <ClinVarAccession Accession="SCV000748419" DateUpdated="2018-05-21" DateCreated="2018-05-21" Type="SCV" Version="3" SubmitterName="GeneDx" OrgID="26957" OrganizationCategory="laboratory" OrgAbbreviation="GDX" />
