        <ClinVarAccession Accession="SCV001588904" DateUpdated="2024-02-14" DateCreated="2021-05-10" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
