        <ClinVarAccession Accession="SCV000882836" DateUpdated="2020-09-19" DateCreated="2019-02-18" Type="SCV" Version="2" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2020-09-16">
          <ReviewStatus>no assertion criteria provided</ReviewStatus>
