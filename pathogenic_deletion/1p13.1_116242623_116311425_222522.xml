        <ClinVarAccession Accession="SCV003866688" DateUpdated="2024-05-01" DateCreated="2023-04-15" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
