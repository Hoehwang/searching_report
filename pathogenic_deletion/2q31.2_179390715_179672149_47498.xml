        <ClinVarAccession Accession="SCV003524860" DateUpdated="2024-02-28" DateCreated="2023-02-07" Type="SCV" Version="2" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
