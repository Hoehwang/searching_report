        <ClinVarAccession Accession="SCV003883327" DateUpdated="2024-05-01" DateCreated="2023-04-15" Type="SCV" Version="3" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
