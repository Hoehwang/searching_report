        <ClinVarAccession Accession="SCV001505302" DateUpdated="2024-06-09" DateCreated="2021-03-14" Type="SCV" Version="5" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
