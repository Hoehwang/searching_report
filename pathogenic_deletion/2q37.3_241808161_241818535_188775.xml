        <ClinVarAccession Accession="SCV000933919" DateUpdated="2024-02-14" DateCreated="2019-08-14" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
