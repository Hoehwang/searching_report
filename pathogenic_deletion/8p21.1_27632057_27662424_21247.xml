        <ClinVarAccession Accession="SCV000778962" DateUpdated="2018-04-02" DateCreated="2018-04-02" Type="SCV" Version="2" SubmitterName="GeneDx" OrgID="26957" OrganizationCategory="laboratory" OrgAbbreviation="GDX" />
