        <ClinVarAccession Accession="SCV001582655" DateUpdated="2024-02-28" DateCreated="2021-05-10" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
