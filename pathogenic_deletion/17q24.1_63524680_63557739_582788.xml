        <ClinVarAccession Accession="SCV000836023" DateUpdated="2024-02-20" DateCreated="2018-10-10" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
