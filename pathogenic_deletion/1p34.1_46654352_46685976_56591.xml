        <ClinVarAccession Accession="SCV004569165" DateUpdated="2024-02-28" DateCreated="2024-02-28" Type="SCV" Version="1" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
