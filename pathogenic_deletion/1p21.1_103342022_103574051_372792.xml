      <ClinicalAssertion ID="38956" SubmissionDate="2019-08-12" DateLastUpdated="2019-08-14" DateCreated="2013-04-04">
        <ClinVarSubmissionID localKey="120280.0005_STICKLER SYNDROME, TYPE II" title="COL11A1, 54-BP EX DEL, AS, AG-TG_STICKLER SYNDROME, TYPE II" />
        <ClinVarAccession Accession="SCV000038956" DateUpdated="2019-08-14" DateCreated="2013-04-04" Type="SCV" Version="3" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="1999-10-01">
