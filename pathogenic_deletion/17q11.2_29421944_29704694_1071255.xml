        <ClinVarAccession Accession="SCV005048774" DateUpdated="2024-06-09" DateCreated="2024-06-09" Type="SCV" Version="1" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
