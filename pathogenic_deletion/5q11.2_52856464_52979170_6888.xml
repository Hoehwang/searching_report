        <ClinVarAccession Accession="SCV000566768" DateUpdated="2017-04-27" DateCreated="2017-04-27" Type="SCV" Version="4" SubmitterName="GeneDx" OrgID="26957" OrganizationCategory="laboratory" OrgAbbreviation="GDX" />
