        <ClinVarAccession Accession="SCV002231513" DateUpdated="2023-02-07" DateCreated="2022-03-28" Type="SCV" Version="2" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2021-07-27">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
