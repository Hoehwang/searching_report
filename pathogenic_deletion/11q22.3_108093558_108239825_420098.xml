        <ClinVarAccession Accession="SCV000622530" DateUpdated="2024-02-20" DateCreated="2017-12-26" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
