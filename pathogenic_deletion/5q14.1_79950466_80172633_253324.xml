        <ClinVarAccession Accession="SCV002752192" DateUpdated="2024-05-01" DateCreated="2022-11-29" Type="SCV" Version="3" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
