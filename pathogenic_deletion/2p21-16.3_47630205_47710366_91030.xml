        <ClinVarAccession Accession="SCV000580441" DateUpdated="2024-05-01" DateCreated="2017-06-24" Type="SCV" Version="6" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-01-24">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Pathogenic</GermlineClassification>
          <Citation>
            <ID Source="PubMed">11579115</ID>
          </Citation>
          <Citation>
