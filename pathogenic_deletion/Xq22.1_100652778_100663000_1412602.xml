        <ClinVarAccession Accession="SCV003997142" DateUpdated="2024-05-01" DateCreated="2023-07-08" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-05-10">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
