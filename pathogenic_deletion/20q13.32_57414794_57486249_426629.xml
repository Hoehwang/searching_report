        <ClinVarAccession Accession="SCV004101280" DateUpdated="2023-11-11" DateCreated="2023-11-11" Type="SCV" Version="1" SubmitterName="Illumina Laboratory Services, Illumina" OrgID="504895" OrganizationCategory="laboratory" />
