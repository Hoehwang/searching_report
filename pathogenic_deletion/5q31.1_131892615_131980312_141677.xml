        <ClinVarAccession Accession="SCV000185136" DateUpdated="2024-05-01" DateCreated="2014-08-06" Type="SCV" Version="9" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
