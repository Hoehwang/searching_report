        <ClinVarAccession Accession="SCV002818522" DateUpdated="2023-01-15" DateCreated="2023-01-15" Type="SCV" Version="1" SubmitterName="ClinGen PAH Variant Curation Expert Panel" OrgID="506558" OrganizationCategory="consortium" OrgAbbreviation="PAH_EP" />
