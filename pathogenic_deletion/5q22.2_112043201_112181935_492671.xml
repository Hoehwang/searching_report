        <ClinVarAccession Accession="SCV004840128" DateUpdated="2024-04-20" DateCreated="2024-04-20" Type="SCV" Version="1" SubmitterName="All of Us Research Program, National Institutes of Health" OrgID="509529" OrganizationCategory="consortium" />
        <RecordStatus>current</RecordStatus>
