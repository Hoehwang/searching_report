        <ClinVarAccession Accession="SCV000739724" DateUpdated="2024-05-01" DateCreated="2018-04-14" Type="SCV" Version="5" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2020-09-14">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Likely pathogenic</GermlineClassification>
          <Citation>
