        <ClinVarAccession Accession="SCV000825846" DateUpdated="2024-02-14" DateCreated="2018-10-10" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
