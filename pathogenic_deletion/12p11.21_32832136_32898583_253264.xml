        <ClinVarAccession Accession="SCV000298167" DateUpdated="2016-08-30" DateCreated="2016-08-30" Type="SCV" Version="1" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
