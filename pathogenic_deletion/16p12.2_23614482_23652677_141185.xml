        <ClinVarAccession Accession="SCV000184361" DateUpdated="2024-05-01" DateCreated="2014-08-06" Type="SCV" Version="8" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
