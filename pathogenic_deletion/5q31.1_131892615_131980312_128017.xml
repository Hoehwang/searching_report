        <ClinVarAccession Accession="SCV000186501" DateUpdated="2024-05-01" DateCreated="2014-08-06" Type="SCV" Version="8" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2021-08-07">
