        <ClinVarAccession Accession="SCV000915988" DateUpdated="2019-05-27" DateCreated="2019-05-27" Type="SCV" Version="1" SubmitterName="Illumina Laboratory Services, Illumina" OrgID="504895" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2018-10-24">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Likely pathogenic</GermlineClassification>
          <Citation>
            <ID Source="PubMed">25721401</ID>
          </Citation>
          <Citation>
            <ID Source="PubMed">25326637</ID>
          </Citation>
