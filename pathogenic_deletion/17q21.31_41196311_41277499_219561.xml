        <ClinVarAccession Accession="SCV000259491" DateUpdated="2016-02-02" DateCreated="2016-02-02" Type="SCV" Version="2" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
