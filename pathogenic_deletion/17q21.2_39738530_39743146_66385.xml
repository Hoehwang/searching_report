        <ClinVarAccession Accession="SCV000035994" DateUpdated="2021-10-30" DateCreated="2013-04-04" Type="SCV" Version="4" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2000-09-01">
