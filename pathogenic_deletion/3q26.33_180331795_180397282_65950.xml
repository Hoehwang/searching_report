        <ClinVarAccession Accession="SCV002724909" DateUpdated="2024-05-01" DateCreated="2022-11-29" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2021-03-11">
