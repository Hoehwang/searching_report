        <Classification DateLastEvaluated="2024-01-11">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Pathogenic</GermlineClassification>
          <Citation>
            <ID Source="PubMed">23807571</ID>
          </Citation>
          <Citation>
            <ID Source="PubMed">25614872</ID>
          </Citation>
          <Citation>
            <ID Source="PubMed">15196260</ID>
          </Citation>
          <Citation>
