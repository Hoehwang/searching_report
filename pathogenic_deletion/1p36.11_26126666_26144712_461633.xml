        <ClinVarAccession Accession="SCV000634411" DateUpdated="2024-02-20" DateCreated="2017-12-26" Type="SCV" Version="9" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
