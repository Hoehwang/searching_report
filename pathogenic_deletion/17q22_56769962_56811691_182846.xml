        <ClinVarAccession Accession="SCV000671917" DateUpdated="2024-05-01" DateCreated="2018-01-01" Type="SCV" Version="5" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-11-27">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Likely pathogenic</GermlineClassification>
          <Citation>
            <ID Source="PubMed">22725699</ID>
          </Citation>
