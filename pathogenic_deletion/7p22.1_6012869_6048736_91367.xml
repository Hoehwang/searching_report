        <ClinVarAccession Accession="SCV004831700" DateUpdated="2024-04-20" DateCreated="2024-04-20" Type="SCV" Version="1" SubmitterName="All of Us Research Program, National Institutes of Health" OrgID="509529" OrganizationCategory="consortium" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-11-08">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
