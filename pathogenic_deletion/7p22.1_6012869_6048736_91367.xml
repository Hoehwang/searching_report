        <ClinVarAccession Accession="SCV004294445" DateUpdated="2024-02-14" DateCreated="2024-02-14" Type="SCV" Version="1" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
