        <ClinVarAccession Accession="SCV002670744" DateUpdated="2024-05-01" DateCreated="2022-11-29" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2021-09-07">
