        <ClinVarAccession Accession="SCV000850117" DateUpdated="2024-05-01" DateCreated="2018-11-08" Type="SCV" Version="5" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
