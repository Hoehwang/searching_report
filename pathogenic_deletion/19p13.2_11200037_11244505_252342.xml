        <ClinVarAccession Accession="SCV001214561" DateUpdated="2024-02-14" DateCreated="2020-04-15" Type="SCV" Version="5" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
