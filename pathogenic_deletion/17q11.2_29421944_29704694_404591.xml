        <ClinVarAccession Accession="SCV000542201" DateUpdated="2024-02-20" DateCreated="2017-04-17" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
