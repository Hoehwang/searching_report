        <ClinVarAccession Accession="SCV001231712" DateUpdated="2024-02-28" DateCreated="2020-04-15" Type="SCV" Version="3" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
