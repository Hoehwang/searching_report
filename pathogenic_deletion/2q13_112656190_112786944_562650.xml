        <ClinVarAccession Accession="SCV000809646" DateUpdated="2022-02-13" DateCreated="2018-09-30" Type="SCV" Version="4" SubmitterName="Quest Diagnostics Nichols Institute San Juan Capistrano" OrgID="500110" OrganizationCategory="laboratory" />
