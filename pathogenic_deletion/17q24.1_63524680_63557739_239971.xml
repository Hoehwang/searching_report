          <Attribute Type="AssertionMethod">ARUP Molecular Germline Variant Investigation Process 2021</Attribute>
          <Citation>
            <URL>https://submit.ncbi.nlm.nih.gov/ft/byid/ythvkvge/arup_molecular_germline_variant_investigation_process_2021.pdf</URL>
          </Citation>
        </AttributeSet>
