        <ClinVarAccession Accession="SCV001347420" DateUpdated="2024-02-14" DateCreated="2020-03-25" Type="SCV" Version="3" SubmitterName="Color Diagnostics, LLC DBA Color Health" OrgID="505849" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-10-05">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
