        <ClinVarAccession Accession="SCV000282988" DateUpdated="2024-02-14" DateCreated="2016-07-01" Type="SCV" Version="10" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
