        <ClinVarAccession Accession="SCV000329474" DateUpdated="2016-12-06" DateCreated="2016-12-06" Type="SCV" Version="5" SubmitterName="GeneDx" OrgID="26957" OrganizationCategory="laboratory" OrgAbbreviation="GDX" />
