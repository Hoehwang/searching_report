        <ClinVarAccession Accession="SCV001362673" DateUpdated="2020-06-22" DateCreated="2020-06-22" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2019-07-06">
