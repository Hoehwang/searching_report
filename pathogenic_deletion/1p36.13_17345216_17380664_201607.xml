        <ClinVarAccession Accession="SCV001585578" DateUpdated="2024-02-28" DateCreated="2021-05-10" Type="SCV" Version="3" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
