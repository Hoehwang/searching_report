        <ClinVarAccession Accession="SCV003934610" DateUpdated="2023-06-24" DateCreated="2023-06-24" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-05-19">
