        <ClinVarAccession Accession="SCV000239240" DateUpdated="2017-12-19" DateCreated="2015-07-18" Type="SCV" Version="3" SubmitterName="GeneDx" OrgID="26957" OrganizationCategory="laboratory" OrgAbbreviation="GDX" />
