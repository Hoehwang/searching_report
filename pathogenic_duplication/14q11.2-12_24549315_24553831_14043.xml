        <ClinVarAccession Accession="SCV000035344" DateUpdated="2015-10-11" DateCreated="2013-04-04" Type="SCV" Version="2" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2007-06-01">
