        <ClinVarAccession Accession="SCV000580849" DateUpdated="2024-05-01" DateCreated="2017-07-01" Type="SCV" Version="6" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
