        <ClinVarAccession Accession="SCV000822817" DateUpdated="2024-02-14" DateCreated="2018-10-10" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
