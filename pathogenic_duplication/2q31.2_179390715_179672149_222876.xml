        <ClinVarAccession Accession="SCV000642669" DateUpdated="2024-02-28" DateCreated="2017-12-26" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2024-01-08">
