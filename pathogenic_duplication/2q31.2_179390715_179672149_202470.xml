        <ClinVarAccession Accession="SCV000827611" DateUpdated="2024-02-28" DateCreated="2018-10-10" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-11-13">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Likely pathogenic</GermlineClassification>
          <Citation>
            <ID Source="PubMed">27353043</ID>
