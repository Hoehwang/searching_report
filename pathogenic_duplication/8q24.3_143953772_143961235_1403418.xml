        <ClinVarAccession Accession="SCV000021384" DateUpdated="2013-04-04" DateCreated="2013-04-04" Type="SCV" Version="1" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="1992-11-01">
