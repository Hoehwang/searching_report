        <ClinVarAccession Accession="SCV000023517" DateUpdated="2018-09-29" DateCreated="2013-04-04" Type="SCV" Version="4" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
