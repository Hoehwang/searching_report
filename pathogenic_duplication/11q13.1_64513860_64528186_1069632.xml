        <ClinVarAccession Accession="SCV004022487" DateUpdated="2024-07-29" DateCreated="2023-08-05" Type="SCV" Version="2" SubmitterName="Practice for Gait Abnormalities, David Pomarino, Competency Network Toe Walking c/o Practice Pomarino" OrgID="507943" OrganizationCategory="consortium" OrgAbbreviation="Praxis Pomarino" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-01-10">
