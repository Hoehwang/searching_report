        <ClinVarAccession Accession="SCV001388779" DateUpdated="2023-02-07" DateCreated="2020-07-16" Type="SCV" Version="3" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
