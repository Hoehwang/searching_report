        <ClinVarAccession Accession="SCV001418173" DateUpdated="2024-02-14" DateCreated="2020-07-16" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
