        <ClinVarAccession Accession="SCV001588207" DateUpdated="2024-02-14" DateCreated="2021-05-10" Type="SCV" Version="3" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-04-26">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
