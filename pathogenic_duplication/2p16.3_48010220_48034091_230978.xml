        <ClinVarAccession Accession="SCV000274691" DateUpdated="2024-05-01" DateCreated="2016-05-29" Type="SCV" Version="9" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-08-08">
