        <ClinVarAccession Accession="SCV000154698" DateUpdated="2016-05-05" DateCreated="2014-06-09" Type="SCV" Version="2" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2014-02-01">
          <ReviewStatus>no assertion criteria provided</ReviewStatus>
