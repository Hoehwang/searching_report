        <ClinVarAccession Accession="SCV000261171" DateUpdated="2024-02-28" DateCreated="2016-01-31" Type="SCV" Version="8" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
