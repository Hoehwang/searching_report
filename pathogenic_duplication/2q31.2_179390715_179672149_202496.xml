        <ClinVarAccession Accession="SCV000821440" DateUpdated="2024-02-28" DateCreated="2018-10-10" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
