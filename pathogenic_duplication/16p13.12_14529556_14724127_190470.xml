        <ClinVarAccession Accession="SCV000830311" DateUpdated="2024-02-28" DateCreated="2018-10-10" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-11-24">
