        <ClinVarAccession Accession="SCV000581834" DateUpdated="2017-07-02" DateCreated="2017-07-02" Type="SCV" Version="4" SubmitterName="GeneDx" OrgID="26957" OrganizationCategory="laboratory" OrgAbbreviation="GDX" />
