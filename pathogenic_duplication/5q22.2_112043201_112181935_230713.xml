        <ClinVarAccession Accession="SCV000274357" DateUpdated="2024-05-01" DateCreated="2016-05-29" Type="SCV" Version="7" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-10-31">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
          <GermlineClassification>Pathogenic</GermlineClassification>
          <Citation>
            <ID Source="PubMed">17604324</ID>
