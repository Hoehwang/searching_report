        <ClinVarAccession Accession="SCV000709818" DateUpdated="2018-04-02" DateCreated="2018-04-02" Type="SCV" Version="1" SubmitterName="GeneDx" OrgID="26957" OrganizationCategory="laboratory" OrgAbbreviation="GDX" />
