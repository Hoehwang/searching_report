        <ClinVarAccession Accession="SCV000630087" DateUpdated="2017-12-26" DateCreated="2017-12-26" Type="SCV" Version="1" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
