        <ClinVarAccession Accession="SCV000634265" DateUpdated="2024-02-20" DateCreated="2017-12-26" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
