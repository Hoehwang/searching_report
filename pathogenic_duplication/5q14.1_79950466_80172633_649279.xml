        <ClinVarAccession Accession="SCV000944071" DateUpdated="2024-02-14" DateCreated="2019-08-14" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
