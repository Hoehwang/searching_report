        <ClinVarAccession Accession="SCV001562968" DateUpdated="2024-02-14" DateCreated="2021-04-13" Type="SCV" Version="3" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
