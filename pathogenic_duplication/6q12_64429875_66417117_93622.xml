        <ClinVarAccession Accession="SCV001591317" DateUpdated="2024-02-14" DateCreated="2021-05-10" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-10-06">
