        <ClinVarAccession Accession="SCV000958032" DateUpdated="2024-02-14" DateCreated="2019-08-14" Type="SCV" Version="6" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2024-01-05">
