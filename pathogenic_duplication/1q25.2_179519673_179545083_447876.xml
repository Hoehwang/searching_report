        <ClinVarAccession Accession="SCV001983641" DateUpdated="2021-10-30" DateCreated="2021-10-30" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2021-09-12">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
