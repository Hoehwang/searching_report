        <ClinVarAccession Accession="SCV000760912" DateUpdated="2024-02-14" DateCreated="2018-05-28" Type="SCV" Version="5" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
