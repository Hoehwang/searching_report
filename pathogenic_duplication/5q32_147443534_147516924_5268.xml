        <ClinVarAccession Accession="SCV001374003" DateUpdated="2023-02-07" DateCreated="2020-07-16" Type="SCV" Version="4" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-06-26">
