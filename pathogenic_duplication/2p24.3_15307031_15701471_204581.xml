        <ClinVarAccession Accession="SCV002766389" DateUpdated="2022-12-24" DateCreated="2022-12-24" Type="SCV" Version="1" SubmitterName="Women's Health and Genetics/Laboratory Corporation of America, LabCorp" OrgID="500026" OrganizationCategory="laboratory" OrgAbbreviation="WHG-LC" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2022-11-05">
          <ReviewStatus>criteria provided, single submitter</ReviewStatus>
