        <ClinVarAccession Accession="SCV001229545" DateUpdated="2024-02-20" DateCreated="2020-04-15" Type="SCV" Version="5" SubmitterName="Invitae" OrgID="500031" OrganizationCategory="laboratory" OrgAbbreviation="Invitae" />
