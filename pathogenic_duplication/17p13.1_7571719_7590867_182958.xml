        <ClinVarAccession Accession="SCV000675338" DateUpdated="2024-05-01" DateCreated="2015-02-24" Type="SCV" Version="6" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" />
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2023-08-14">
