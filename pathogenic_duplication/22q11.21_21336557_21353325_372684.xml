        <ClinVarAccession Accession="SCV002749753" DateUpdated="2024-06-09" DateCreated="2022-11-29" Type="SCV" Version="2" SubmitterName="Ambry Genetics" OrgID="61756" OrganizationCategory="laboratory" OrgAbbreviation="Ambry" />
